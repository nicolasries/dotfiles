[alias]
    l = log --graph --pretty=format:'%Cred%h%Creset %C(yellow)%an%d%Creset %s %Cgreen(%cr)%Creset' --date=relative
    d = diff
    c = commit
    s = !git status && echo && git submodule foreach 'git status && echo '
    p = pull
    pall = !git pull && git submodule foreach 'git pull'
[include]
    path = config.local
